# $Id$

RM=@RM@
INSTALL=@INSTALL@
INSTALL_DATA=@INSTALL_DATA@
PREFIX=@prefix@

SUBDIRS=alis main operbot chanserv userserv jupeserv operserv

install: 
	$(INSTALL) -m 750 -d $(PREFIX)/help

	@for i in $(SUBDIRS); do \
		$(INSTALL) -m 750 -d $(PREFIX)/help/$$i; \
		for j in $$i/*; do \
			if [ -f $$j ]; then \
				$(INSTALL_DATA) $$j $(PREFIX)/help/$$i; \
			fi \
		done \
	done

build:
clean:
depend:

distclean:
	$(RM) -f Makefile

