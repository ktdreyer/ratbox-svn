CC = @CC@
CFLAGS = @CFLAGS@

# Debugging CFLAGS
#CFLAGS = -g -Wall

all: source
	@cd source ; $(MAKE) "CC=$(CC)" "CFLAGS=$(CFLAGS)"

build: all

clean:
	@cd source ; $(MAKE) clean

distclean: clean
	rm -f Makefile source/Makefile

depend:
	@cd source ; $(MAKE) depend

# End of Makefile
